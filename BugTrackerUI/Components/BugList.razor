<h3>All bugs</h3>

@code {

    [Inject]
    public IBugService BugService { get; set; }


    public List<Bug> Bugs { get; set; }

    [Inject]
    public NavigationManager NavService { get; set; }

    protected override void OnInitialized()
    {
        BugService.GetBugs().OrderBy(x => x.Priority);
    }

}
